# Setup default flags for the compiler.
env_CFLAGS="$CFLAGS"
case $target:$blt_have_gcc:$blt_enable_symbols in
  *-solaris2*:no:yes)
    CFLAGS="$env_CFLAGS -g -xs -O"
    ;;
  *:yes:no)
    CFLAGS="$env_CFLAGS -O3"
    ;;
  *:yes:yes)
    CFLAGS="$env_CFLAGS -g -O2"
    ;;
  *:no:no)
    CFLAGS="$env_CFLAGS -O"
    ;;
  *)
    CFLAGS="$env_CFLAGS -g -O2"
    ;;
esac

